{
  "kind": "collectionType",
  "collectionName": "tours",
  "info": {
    "singularName": "tour",
    "pluralName": "tours",
    "displayName": "Tour",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "shortDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "brochure": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tourCard": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "tour.tour-card"
    },
    "destination": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::destination.destination"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title"
    },
    "scheduleImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "mapImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tourCustom": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "common.feature",
        "common.icon-value",
        "common.list-contact",
        "common.list-features",
        "common.list-with-icon-value",
        "common.custom-ck-editor"
      ]
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "common.feature"
    },
    "byg": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "paymentProduct": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::payment-vns.payment-product",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "places": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::place.place"
    },
    "adultPrice": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "childPrice": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "isHot": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "discountLabel": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    }
  }
}
